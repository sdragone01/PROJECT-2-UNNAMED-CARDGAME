{% layout "layout.liquid" %} {% block content %}
<div class="top">
    <h1>Cards</h1>
  
    <a href="/home"><button>Home</button></a>
</div>
<div>
  
    <a href="/cards/new"><button>Create A New card</button></a>
  {% for card in cards %}
  <article>
    <a href="/cards/{{ card._id }}">
      <h2>
        {{card.name}} 
      </h2>
    </a>
  </article>
  {% endfor %}
</div>
{% endblock %}